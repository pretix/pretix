      "invoice_text": {
        "description": "Text on invoice (localized)",
        "type": ["object", "null"],
        "patternProperties": {
          "[a-zA-Z-]+": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ORDER": {
        "description": "Internal, for backwards-compatibility, will be ignored.",
        "type": "number"
      },
      "DELETE": {
        "description": "Internal, for backwards-compatibility, will be ignored.",
        "type": "boolean"
      }
    },
    "required": ["country", "address_type", "action"],
    "additionalProperties": false
  }
