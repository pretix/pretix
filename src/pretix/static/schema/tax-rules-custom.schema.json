        "enum": ["vat", "reverse", "no", "block", "require_approval"]
      },
      "rate": {
        "description": "Tax rate in case of action=vat or action=require_approval (or null for default)",
        "type": ["string", "null"],
        "pattern": "^[0-9]+(\\.[0-9]+)?$"
